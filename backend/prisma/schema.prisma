datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

model SecretModel {
  id     Int    @id @default(autoincrement())
  name   String @db.VarChar(255)
  secret String @db.VarChar(255)
}
